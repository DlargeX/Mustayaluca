<?xml version="1.0" encoding="utf-8" standalone="yes"?>
<window>
  <id>600</id>
  <defaultcontrol>2</defaultcontrol>
  <allowoverlay>no</allowoverlay>
  <controls>
    <control>
      <description>BG</description>
      <type>image</type>
      <id>0</id>
      <width>1920</width>
      <height>1080</height>
      <texture>backgrounds\tv\default.jpg</texture>
    </control>
    <control>
      <description>Backdrop Overlay</description>
      <type>image</type>
      <id>0</id>
      <posX>0</posX>
      <posY>0</posY>
      <width>1925</width>
      <height>1085</height>
      <texture>overlay_background.png</texture>
    </control>
    <control>
      <description>Backdrop Overlay - Filmstrip/Coverflow</description>
      <type>image</type>
      <id>1</id>
      <posX>0</posX>
      <posY>0</posY>
      <width>1925</width>
      <height>1085</height>
      <texture>overlay_background_top.png</texture>
    </control>
    <control Style="BigTitle">
      <description>Guide Group Title</description>
      <type>label</type>
      <id>11</id>
      <posX>70</posX>
      <posY>45</posY>
      <width>1220</width>
      <label>#TV.Guide.Group</label>
      <visible>!player.hasmedia</visible>
    </control>
    <control Style="Time">
      <description>Date</description>
      <type>label</type>
      <id>0</id>
      <posX>70</posX>
      <posY>120</posY>
      <label>#date / #time</label>
    </control>
    <control Style="BigTitle">
      <description>Single Channel Label</description>
      <type>label</type>
      <id>32</id>
      <posX>1760</posX>
      <posY>45</posY>
      <width>400</width>
      <align>right</align>
    </control>
    <control>
      <description>Single Channel Image</description>
      <type>image</type>
      <id>33</id>
      <posX>1780</posX>
      <posY>42</posY>
      <width>70</width>
      <height>70</height>
      <zoom>no</zoom>
      <centered>yes</centered>
      <keepaspectratio>yes</keepaspectratio>
      <texture>DefaultMyEPGBig.png</texture>
    </control>
    <!-- Timeheader background must be dedfined before Timeheader label template -->
    <!-- b/c Timeheader is on the background and will be rendered first -->
    <control>
      <description>time header background image template</description>
      <type>image</type>
      <id>90</id>
      <posX>434</posX>
      <posY>190</posY>
      <width>100</width>
      <height>30</height>
      <texture>-</texture>
    </control>
    <control>
      <description>time header label template</description>
      <type>label</type>
      <id>40</id>
      <posX>434</posX>
      <posY>190</posY>
      <font>font2</font>
      <textcolor>ffa9d0f7</textcolor>
    </control>
    <control>
      <description>day spin control</description>
      <type>spincontrol</type>
      <id>6</id>
      <posX>140</posX>
      <posY>190</posY>
      <width>40</width>
      <height>30</height>
      <showrange>no</showrange>
      <orientation>horizontal</orientation>
      <align>center</align>
      <onup>21</onup>
      <onleft>60</onleft>
      <onright>8</onright>
      <textcolor>ffa9d0f7</textcolor>
      <textureUp>arrow_round_right_nofocus.png</textureUp>
      <textureDown>arrow_round_left_nofocus.png</textureDown>
      <textureUpFocus>arrow_round_right_focus.png</textureUpFocus>
      <textureDownFocus>arrow_round_left_focus.png</textureDownFocus>
    </control>
    <control>
      <description>time interval spin control</description>
      <type>spincontrol</type>
      <id>8</id>
      <posX>340</posX>
      <posY>190</posY>
      <width>40</width>
      <height>30</height>
      <showrange>no</showrange>
      <orientation>horizontal</orientation>
      <align>center</align>
      <textureUp>arrow_round_right_nofocus.png</textureUp>
      <textureDown>arrow_round_left_nofocus.png</textureDown>
      <textureUpFocus>arrow_round_right_focus.png</textureUpFocus>
      <textureDownFocus>arrow_round_left_focus.png</textureDownFocus>
      <onup>21</onup>
      <onleft>6</onleft>
      <onright>8</onright>
    </control>
    <import tag="TVGuideChannelTemplate">mytvguide.common.10rows.channeltemplate.xml</import>
    <control Style="NoShadow">
      <description>Current program title</description>
      <type>fadelabel</type>
      <id>13</id>
      <posX>340</posX>
      <posY>840</posY>
      <width>410</width>
      <font>font6</font>
      <width>1500</width>
      <label>#TV.Guide.CompositeTitle</label>
    </control>
    <control Style="NoShadow">
      <description>Current program time</description>
      <type>label</type>
      <id>14</id>
      <posX>340</posX>
      <posY>900</posY>
      <font>font2</font>
      <width>1500</width>
      <textcolor>ffa9d0f7</textcolor>
      <label>#TV.Guide.Time   #TV.Guide.ChannelName   #TV.Guide.DurationMins   #TV.Guide.Genre</label>
    </control>
    <control Style="NoShadow">
      <description>Current program genre</description>
      <type>label</type>
      <id>27</id>
      <posX>-2000</posX>
      <posY>940</posY>
      <width>500</width>
      <font>font2</font>
      <textcolor>ffa9d0f7</textcolor>
      <label>-</label>
    </control>
    <control>
      <description>Description1</description>
      <type>textboxscrollup</type>
      <id>15</id>
      <posX>340</posX>
      <posY>940</posY>
      <width>1500</width>
      <height>125</height>
      <font>font14</font>
      <label>#TV.Guide.Description</label>
      <textcolor>ffcdcccc</textcolor>
    </control>
    <control>
      <description>vertical timeline</description>
      <type>image</type>
      <id>25</id>
      <posX>50</posX>
      <posY>222</posY>
      <height>450</height>
      <colordiffuse>ffffffff</colordiffuse>
      <texture>tvguide_slider_new.png</texture>
    </control>
    <control>
      <description>TV background</description>
      <type>image</type>
      <id>98</id>
      <posX>2000</posX>
      <posY>10</posY>
      <width>10</width>
      <height>10</height>
      <texture>-</texture>
      <visible>no</visible>
    </control>
    <control>
      <description>TV preview window</description>
      <type>videowindow</type>
      <id>99</id>
      <posX>2000</posX>
      <posY>10</posY>
      <width>10</width>
      <height>10</height>
      <texture>-</texture>
      <visible>no</visible>
      <action>18</action>
    </control>
    <control>
      <description>record pin</description>
      <type>image</type>
      <id>31</id>
      <posX>210</posX>
      <posY>850</posY>
      <align>left</align>
      <texture>tvguide_record_button.png</texture>
      <visible>no</visible>
    </control>
    <control>
      <description>TvGroup button label</description>
      <type>label</type>
      <id>0</id>
      <posX>112</posX>
      <posY>250</posY>
      <label>#TV.Guide.ChangeGroup</label>
      <font>font2</font>
      <textcolor>FFFFFFFF</textcolor>
      <animation effect="rotate" end="-90" time="0">WindowOpen</animation>
    </control>
    <control>
      <description>Template for genre key color</description>
      <type>image</type>
      <id>110</id>
      <posX>650</posX>
      <posY>125</posY>
      <width>36</width>
      <height>18</height>
      <texture overlay="tvguide_button_middle_colorize_overlay.png">tvguide_button_middle_colorize.png</texture>
      <visible>skin.hassetting(#skin.tvguide.showgenrekey)</visible>
    </control>
    <control Style="Time">
      <description>Template for genre key name</description>
      <type>fadelabel</type>
      <id>111</id>
      <posY>120</posY>
      <width>120</width>
      <visible>skin.hassetting(#skin.tvguide.showgenrekey)</visible>
    </control>
    <import>common.overlay.home.xml</import>
  </controls>
</window>
